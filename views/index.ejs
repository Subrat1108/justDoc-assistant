<!DOCTYPE html>
<html ng-app="justDoc">
  <head>
      <meta charset="utf-8">
      <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  
      <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"></script>
      
      <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.6/angular.min.js"></script>
      <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.6/angular-route.js"></script>
      <script src ="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.6/angular-cookies.js"></script>
      <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" ></script>

      <script src="/javascripts/justDoc.js"></script>

    <title>JustDoc</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body ng-controller="mainController">
    <h1></h1>
    <p>Welcome to JustDoc Assistant</p>

    <form ng-submit="submit()">
      <input type="text" aria-label="Name" placeholder="name" ng-model="answers.name"><br>
      <input type="number" aria-label="Age" placeholder="age" ng-model="answers.age"><br>
      <h3>Symptoms</h3>
      <p>Fever</p><input type ="checkbox" aria-label="fever" ng-model="answers.symptoms.fever"><br>
      <p>Head Ache</p><input type="checkbox" aria-label="headache" ng-model="answers.symptoms.headache"><br>
      <input type="submit">
    </form>

    


  </body>
</html>
